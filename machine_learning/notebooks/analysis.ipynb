    "# Exemplos de predi√ß√µes em tempo real\n",
    "print("üîÆ EXEMPLOS DE PREDI√á√ïES EM TEMPO REAL")\n",
    "print("=" * 60)\n",
    "\n",
    "# Selecionando 5 amostras aleat√≥rias\n",
    "sample_indices = np.random.choice(len(X_test), 5, replace=False)\n",
    "\n",
    "for i, idx in enumerate(sample_indices):\n",
    "    # Dados reais\n",
    "    real_features = X_test.iloc[idx]\n",
    "    real_status = label_encoder.classes_[y_test[idx]]\n",
    "    \n",
    "    # Predi√ß√£o\n",
    "    pred_features = X_test_scaled[idx].reshape(1, -1)\n",
    "    pred_status_encoded = model.predict(pred_features)[0]\n",
    "    pred_status = label_encoder.classes_[pred_status_encoded]\n",
    "    \n",
    "    # Probabilidades\n",
    "    probabilities = model.predict_proba(pred_features)[0]\n",
    "    confidence = max(probabilities)\n",
    "    \n",
    "    print(f"\nüîß Equipamento {i+1}:")\n",
    "    print(f"üìä Leituras dos Sensores:")\n",
    "    print(f"   ‚Ä¢ Temperatura: {real_features['temperature']:.1f}¬∞C")\n",
    "    print(f"   ‚Ä¢ Umidade: {real_features['humidity']:.1f}%")\n",
    "    print(f"   ‚Ä¢ Press√£o: {real_features['pressure']:.3f} bar")\n",
    "    print(f"   ‚Ä¢ Vibra√ß√£o: {real_features['vibration']:.2f} Hz")\n",
    "    print(f"   ‚Ä¢ Corrente: {real_features['current']:.1f} A")\n",
    "    \n",
    "    print(f"\nüéØ Resultados:")\n",
    "    print(f"   ‚Ä¢ Status Real: {real_status}")\n",
    "    print(f"   ‚Ä¢ Status Predito: {pred_status}")\n",
    "    print(f"   ‚Ä¢ Confian√ßa: {confidence:.1%}")\n",
    "    \n",
    "    # Probabilidades por classe\n",
    "    print(f"   ‚Ä¢ Probabilidades:")\n",
    "    for j, class_name in enumerate(label_encoder.classes_):\n",
    "        print(f"     - {class_name}: {probabilities[j]:.1%}")\n",
    "    \n",
    "    if real_status == pred_status:\n",
    "        print("   ‚úÖ Predi√ß√£o CORRETA")\n",
    "    else:\n",
    "        print("   ‚ùå Predi√ß√£o INCORRETA")\n",
    "    \n",
    "    print("-" * 50)"
